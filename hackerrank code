import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;



public class Solution {
    public static void main(String[] args) {
     Scanner sc = new Scanner(System.in);
     int num= sc.nextInt();
     for(int i=1;i<11;i++)
     {
        int a=i*num;
        System.out.printf("%d x %d = %d\n",num,i,a);
     } 
     sc.close();   
    }
    
}
